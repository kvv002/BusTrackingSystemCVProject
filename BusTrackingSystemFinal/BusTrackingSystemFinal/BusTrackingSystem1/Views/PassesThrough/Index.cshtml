@model IEnumerable<BusTrackingSystem1.Models.passes_through>

@{
    ViewBag.Title = "Index";
}



<p style="padding-top:50px; font-size:20px">
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table" style="color:white">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.order_no)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.time_to_reach)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.location.location_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.route.source)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.route.destination)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.order_no)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.time_to_reach)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.location.location_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.route.source)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.route.destination)
            </td>

            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.passes_through_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.passes_through_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.passes_through_id })
            </td>
        </tr>
    }

</table>
